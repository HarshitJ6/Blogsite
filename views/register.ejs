
<%- include ('partials/header.ejs')%>

<form action="/register" method="POST">
<div class="box col-lg-4 col-sm-9 col-9 col-md-5">
    <h3>REGISTER</h3>
    <label for="email">E-mail/Username</label>
    <input type="email" name="username" id="email">
    <label for="pass">Password</label>
    <input type="password" name="password" id="pass">
    <button class="btn btn-success" type="submit">LOGIN</button><br>
    <p class="optional">Already registered? click <a href="/login">here</a> to login!</p>
</div>
</form>

